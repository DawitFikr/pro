<LoginWindow>:
	name: "login"
	BoxLayout:
		size: root.width, root.height 
		pos: 0,0
		orientation:"vertical"
		
		canvas.before:
			Color:
				rgba: (0/255, 200/255, 200/255, .2)
			Rectangle:
				pos: self.pos
				size: self.size
				
		Label:
			halign: "center"
			valign: "bottom"
			size_hint_y: None
			height: 80
			
		AnchorLayout:
			anchor_x: "center"
			anchor_y: "top"
			
			BoxLayout:
				orientation:"vertical"
				size_hint: .9, .7
				spacing: 5
				
				canvas.before:
					Color:
						rgba: (0/255, 210/255, 210/255, .2)
					Rectangle:
						pos: self.pos
						size: self.size
				
				GridLayout:
					cols:1
					spacing: 5
					padding: 10
					
					BoxLayout:
						orientation:"vertical"
						size_hint: 1, .2
						background_color: (255/255, 255/255, 120/255, 1)	
							
					BoxLayout:
						orientation:"vertical"
						size_hint: 1, 2.5
						background_color: (255/255, 255/255, 120/255, 1)
						Image:
							source: "images/Duli.jpg"
					
					Label:
						text: "...............Digital solutions..............."
						font_size: 40
						text_size: self.size
						halign: "center"
						valign: "top"
						color: (250/255, 255/255, 255/255, 1)
	
					Label:
						text: "Login:"
						font_size: 55
						text_size: self.size
						halign: "center"
						valign: "top"
						
					Label:
						id: error 
						text: ""
						markup: True 
						size_hint_y: 0.4
						text_size: self.size
						halign: "left"
						valign: "top"
						
					TextInput:
						id: name
						hint_text: "Agent Name:"
						size_hint_y: None
						height: 70
						multiline: False 
						
					TextInput:
						id: operator
						hint_text: "Operator ID:"
						size_hint_y: None
						height: 70
						multiline: False 
						
					TextInput:
						id: password 
						hint_text: "Password:"
						size_hint_y: None
						height: 70
						multiline: False 
						password: True 
						
					Button:
						text: "FORGOT PASSWORD?"
						size_hint_y: .7
						background_normal: ""
						background_color: (0/255, 200/255, 200/255, .2)
						color: (202/255, 220/255, 252/255, 1)
						font_size: "15sp"
						on_release:
							root.manager.current = "forgotp"
							root.manager.transition.direction = "up"
						
				BoxLayout:
					size_hint_y: None
					height: 100
					
					AnchorLayout:
						anchor_x: "left"
						anchor_y: "bottom"
						
						Button:
							text: "Log in"
							background_normal: ""
							background_color: (65/255, 255/255, 32/255, 0.7)
							on_release:
								root.worksheet()
								root.login_user()
								
								
					AnchorLayout:
						anchor_x: "right"
						anchor_y: "bottom"
						
						Button:
							text: "Register >>"
							on_release:
								root.manager.current = "register"
	
	
	


